{
    "name": "depa/middleware-session",
    "description": "Session handling middleware for use with zend expressive 2.0 based on zend-session.",
    "keywords": [
        "session handling",
        "psr-7",
        "zend",
        "expressive",
        "middleware"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Wendrich",
            "email": "daniel.wendrich@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Depa\\MiddlewareSession\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Depa\\MiddlewareSessionTest\\": "test/"
        }
    },
    "require": {
        "php": "^7.0",
        "zendframework/zend-session": "^2.7",
        "zendframework/zend-servicemanager": "^3.3",
        "psr/http-message": "^1.0",
        "http-interop/http-middleware": "^0.4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.8",
        "squizlabs/php_codesniffer": "^2.8.1"
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit",
        "test-coverage": "phpunit --color=never --coverage-text"
    }
}
